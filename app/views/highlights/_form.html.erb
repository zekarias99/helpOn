<%= form_for(@highlight) do |f| %>

  <%= render 'shared/error_messages', object: @highlight %>
  <fieldset>
    <p>
      <%= f.label :description %>
      <%= f.text_area :description %>
    </p>
    <p>
      <%= f.label :deal_id %><br />
      <%= f.text_field :deal_id %>
    </p>
        <%= f.submit %>
  </fieldset>
<% end %>
